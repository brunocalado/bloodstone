difiedTime":1662937134075,"lastModifiedBy":"yzwzZZjViAtwZgNJ"},"_id":"nQ6HEg9BYJS3qyRM"},{"name":"Resist","type":"script","scope":"global","author":"yzwzZZjViAtwZgNJ","img":"icons/skills/melee/maneuver-daggers-paired-orange.webp","command":"/*\nParry\nReload\nStun\n*/\n\nmain();\n\nfunction main() {  \n\n  let options = ['1','2','3'];\n  let dicePoolList = ``;\n  options.map((t) => {\n    dicePoolList += `<option value=\"${t}\">${t}</option>`;\n  });  \n\n  let template = `      \n    <style type=\"text/css\">\n    .tg  {border-collapse:collapse;border-spacing:0;}\n    .tg td{border-color:black;border-style:solid;border-width:1px;font-family:Arial, sans-serif;font-size:14px;\n      overflow:hidden;padding:10px 5px;word-break:normal;}\n    .tg th{border-color:black;border-style:solid;border-width:1px;font-family:Arial, sans-serif;font-size:14px;\n      font-weight:normal;overflow:hidden;padding:10px 5px;word-break:normal;}\n    .tg .tg-nrix{text-align:center;vertical-align:middle}\n    </style>\n    \n    <table class=\"tg\">\n    <thead>\n      <tr>\n        <th class=\"tg-nrix\">\n          Dice Pool\n        </th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr>\n        <td class=\"tg-nrix\">\n          <select id=\"dicePool\" type=\"text\" style=\"width: 100px; text-align: center;\">\n            ${dicePoolList}\n          </select>  \n        </td>\n      </tr>\n    </tbody>\n    </table>  \n\n    <br>    \n    <details>\n      <summary>Rules (click to expand)</summary>\n      <p><b>Resist:</b> reduce or avoid Consequences. Describe how you do it, than roll 1d per each Stamina spent.</p>\n    </details> \n    <br>    \n  `;\n  \n  new Dialog({\n    title: `Resist`,\n    content: template,\n    buttons: {\n      ok: {\n        label: \"Resist!!!\",\n        callback: async (html) => {\n          support(html);\n        }\n      },\n      cancel: {\n        label: \"Cancel\"\n      }\n    },\n    default: \"ok\"\n  }, {}).render(true);\n}\n\nasync function support(html) {\n  const dicePool = parseInt( html.find(\"#dicePool\")[0].value );    \n  const diceExpression = `${dicePool}d6kh`;\n  let total;\n  let message=``;\n  let diceRoll;\n    \n  // ---\n  const messageCrit =`<p style=\"color:darkgreen;\">Perfect. You avoid the consequence completely and refresh 1 Blood.</p>`;\n  const messageSuccess =`<p style=\"color:darkgreen;\">Good. You avoid the consequence completely.</p>`;\n  const messagePartial =`<p>Close. You partially reduce the consequence.</p>`;\n  const failed = `<p style=\"color:darkred;\">Bad. You suffer the full consequence.</p>`;\n\n  diceRoll = await new Roll(diceExpression).roll({ async : false });  \n  total = diceRoll.total;\n  const results = diceRoll.dice[0].results;\n  let allRolls = [];\n  for (let i=0; i<results.length; i++) {\n    allRolls.push(results[i].result); \n  }\n  const totalSix = allRolls.filter(x => x==6).length;\n\n  \n  if (totalSix==1) {    \n    message = `<h2>Good!</h2>`;    \n    message += messageCrit;\n  } else if (totalSix>1) {\n    message = `<h2>Perfect!</h2>`;    \n    message += messageSuccess;\n  } else if (total>3) {\n    message = `<h2>Close!</h2>`;    \n    message += messagePartial;\n  } else {\n    message = `<h2>Bad!</h2>`;    \n    message += failed;    \n  }\n  message += `<p>You spent ${dicePool} stamina points.</p>`;                \n  \n  // send message1\n  let chatData = {\n    content: message\n  };  \n  ChatMessage.create(chatData, {});  \n  \n  diceRoll.toMessage();\n}","folder":null,"sort":0,"ownership":{"default":0,"yzwzZZjViAtwZgNJ":3},"flags":{"core":{"sourceId":"Macro.VKtqSrnTwmcgAzzW"}},"_stats":{"systemId":"worldbuilding","systemVersion":"0.7.1","coreVersion":"10.285","createdTime":1663680378047,"modifiedTime":1663709388549,"lastModifiedBy":"yzwzZZjViAtwZgNJ"},"_id":"VKtqSrnTwmcgAzzW"},{"name":"Firearm","type":"script","scope":"global","author":"yzwzZZjViAtwZgNJ","img":"icons/weapons/guns/gun-pistol-brass.webp","command":"/*\nParry\nReload\nStun\n*/\n\nmain();\n\nfunction main() {  \n\n  let options = ['Parry','Reload','Stun'];\n  let optionsList = ``;\n  options.map((t) => {\n    optionsList += `<option value=\"${t}\">${t}</option>`;\n  });\n\n  options = ['Pistol','Blunderbuss','Rifle'];\n  let firearmsList = ``;\n  options.map((t) => {\n    firearmsList += `<option value=\"${t}\">${t}</option>`;\n  });\n\n  options = ['1','2','3','4','5','6'];\n  let dicePoolList = ``;\n  options.map((t) => {\n    dicePoolList += `<option value=\"${t}\">${t}</option>`;\n  });  \n\n  let template = `      \n    <style type=\"text/css\">\n    .tg  {border-collapse:collapse;border-spacing:0;}\n    .tg td{border-color:black;border-style:solid;border-width:1px;font-family:Arial, sans-serif;font-size:14px;\n      overflow:hidden;padding:10px 5px;word-break:normal;}\n    .tg th{border-color:black;border-style:solid;border-width:1px;font-family:Arial, sans-serif;font-size:14px;\n      font-weight:normal;overflow:hidden;padding:10px 5px;word-break:normal;}\n    .tg .tg-nrix{text-align:center;vertical-align:middle}\n    </style>\n\n    <table class=\"tg\">\n    <thead>\n      <tr>\n        <th class=\"tg-nrix\">\n          Firearm\n        </th>\n        <th class=\"tg-nrix\">\n          Action\n        </th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr>\n        <td class=\"tg-nrix\">\n        <select id=\"firearmType\" type=\"text\" style=\"width: 100px; text-align: center;\">\n          ${firearmsList}\n        </select>  \n        </td>\n        <td class=\"tg-nrix\">\n        <select id=\"firearmAction\" type=\"text\" style=\"width: 100px; text-align: center;\">\n          ${optionsList}\n        </select>      \n        </td>\n      </tr>\n      <tr>\n        <td class=\"tg-nrix\" colspan=\"2\">\n          Dice Pool: \n          <select id=\"dicePool\" type=\"text\" style=\"width: 100px; text-align: center;\">\n            ${dicePoolList}\n          </select>           \n        </td>\n      </tr>\n    </tbody>\n    </table>  \n\n    <br>    \n    <details>\n      <summary>Rules (click to expand)</summary>\n      <p><b>Parry:</b> roll 1d per each bullet you mark. With a result equal [Parry] you interrupt an enemy action.</p>\n      <p><b>Stun:</b> roll 1d per every two bullets you mark. With a result equal [Stun] you stun an enemy for a few seconds.</p>\n      <p><b>Reload:</b> mark any amount of Blood and/or Silver Bullets. For each, roll 1d. Regain bullets equal to the highest result.</p>\n    </details> \n    <br>    \n  `;\n  \n  new Dialog({\n    title: `Firearm Manager`,\n    content: template,\n    buttons: {\n      ok: {\n        label: \"Fire!!!\",\n        callback: async (html) => {\n          support(html);\n        }\n      },\n      cancel: {\n        label: \"Cancel\"\n      }\n    },\n    default: \"ok\"\n  }, {}).render(true);\n}\n\nasync function support(html) {\n  const firearmAction = html.find(\"#firearmAction\")[0].value;    \n  const firearmType = html.find(\"#firearmType\")[0].value;    \n  const dicePool = parseInt( html.find(\"#dicePool\")[0].value );    \n  const diceExpression = `${dicePool}d6kh`;\n  let total;\n  let message=``;\n  let diceRoll;\n    \n  // ---\n  const parrySuccess ='<p style=\"color:darkgreen;\">Parry: you interrupt an enemy action.</p>';\n  const stunSuccess ='Stun: you stun an enemy for a few seconds.';\n  const reloadSuccess ='Reload: Regain bullets equal to ';\n  const failed = `<p style=\"color:darkred;\">You failed.</p>`;\n\n  diceRoll = await new Roll(diceExpression).roll({ async : false });  \n  total = diceRoll.total;\n  \n  if (firearmAction=='Parry') {    \n    message = `<h2>Parry (${firearmType})</h2>`;    \n    message += `<p>You rolled <b style=\"color:red;\">${total}</b>.</p>`;        \n    if (firearmType=='Pistol') {\n      if (total>=4) {\n        message += `${parrySuccess}`;    \n      } else {\n        message += failed;    \n      }      \n    } else if (firearmType=='Blunderbuss') {\n      if (total>=6) {\n        message += `<p>${parrySuccess}</p>`;    \n      } else {\n        message += failed;    \n      }           \n    } else if (firearmType=='Rifle') {\n      if (total>=5) {\n        message += `<p>${parrySuccess}</p>`;    \n      } else {\n        message += failed;    \n      }                 \n    }\n    message += `<p>You spent ${dicePool} silver bullets.</p>`;        \n  } else if (firearmAction=='Stun') {\n    message = `<h2>Stun (${firearmType})</h2>`;    \n    message += `<p>You rolled <b style=\"color:red;\">${total}</b>.</p>`;            \n    if (firearmType=='Pistol') {\n      if (total>=6) {\n        message += `<p>${parrySuccess}</p>`;    \n      } else {\n        message += failed;    \n      }     \n    } else if (firearmType=='Blunderbuss') {\n      if (total>=4) {\n        message += `<p>${parrySuccess}</p>`;    \n      } else {\n        message += failed;    \n      }           \n    } else if (firearmType=='Rifle') {\n      if (total>=5) {\n        message += `<p>${parrySuccess}</p>`;    \n      } else {\n        message += failed;    \n      }                 \n    }\n    message += `<p>You spent ${dicePool*2} silver bullets.</p>`;        \n  } else if (firearmAction=='Reload') {\n    message = `<h2>Reload (${firearmType})</h2>`;    \n    message += `<p>You rolled <b style=\"color:red;\">${total}</b>.</p>`;                \n    message += `<p>${reloadSuccess} ${total}.</p>`;    \n  }\n\n  // send message1\n  let chatData = {\n    content: message\n  };  \n  ChatMessage.create(chatData, {});  \n  \n  diceRoll.toMessage();\n}","folder":null,"sort":0,"ownership":{"default":2},"flags":{"core":{"sourceId":"Macro.kiIcArKRDv0up04n"}},"_stats":{"systemId":"worldbuilding","systemVersion":"0.7.1","coreVersion":"10.285","createdTime":1663249555507,"modifiedTime":1663709430186,"lastModifiedBy":"yzwzZZjViAtwZgNJ"},"_id":"kiIcArKRDv0up04n"}],"cards":[],"playlists":[{"name":"Oath of Hunters","description":"","sounds":[{"name":"Oath of Hunters - Portuguese (